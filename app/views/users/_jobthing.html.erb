<div>
   <%= jobthing.company%>   <button type="button" class="btn btn-primary btn-xs job-page-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>">View</button>
  <!-- refactor this crap! -->
  <button id="jobthing-id" style="display: none"><%=jobthing.id%></button>
  <button id="create-applied-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>/applieds"></button>
  <button id="new-interview-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>/interviews/new"></button>
  <button id="create-interview-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>/interviews"></button>
  <button id="new-misc-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>/miscjobthings/new"></button>
  <button id="new-outcome-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>/outcomes/new"></button>
  <button id="delete-jobthing-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>"></button>
  <% if jobthing.has_applied %>
    <button id="delete-applied-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>/applieds/<%=jobthing.applied.id%>"></button>
    <button id="applied-id" style="display: none"><%=jobthing.applied.id%></button>
  <% end %>
  <% if jobthing.interviews %>
    <button id="delete-interviews-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>/destroy_interviews"></button>
  <% end %>
  <% if jobthing.miscjobthings %>
    <button id="delete-miscs-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>/destroy_miscs"></button>
  <% end %>
  <% if jobthing.has_outcome %>
    <button id="delete-outcome-link" style="display: none" action="/users/<%=@user.id%>/jobthings/<%=jobthing.id%>/outcomes/<%=jobthing.outcome.id%>"></button>
    <button id="outcome-id" style="display: none"><%=jobthing.outcome.id%></button>
  <% end %>
</div>
