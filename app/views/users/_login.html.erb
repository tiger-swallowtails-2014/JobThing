
<div class="row loginpadding"></div>
  <div class="row">
    <div class="loginform col-md-4 col-md-offset-2">
      <h2>Welcome Back.</h2>
        <div class="signin">
          <div>
            <%= bootstrap_form_tag url:'/login', method: :post do |f| %><br>
              <%= f.text_field :email, placeholder: "Email" %>
              <%= f.password_field :password, placeholder:"Password" %>
              <%= f.submit "Sign In" %>
            <% end %>
          </div>
      </div>
  </div>
</div>


