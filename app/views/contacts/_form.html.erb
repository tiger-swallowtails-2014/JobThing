<%= form_for [@user, @jobthing, @contact] do |f| %>
First Name: <%= f.text_field :first_name %><br>
Last Name: <%= f.text_field :last_name %><br>
Phone Number: <%= f.text_field :phone_number, placeholder: "xxx-xxx-xxxx" %><br>
Email: <%= f.text_field :email %><br>
<%= f.submit "Submit", class: "contact-submit" %>
<% end %>
